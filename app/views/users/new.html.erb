<h1>Join the Renecades Gameroom!</h1>
 <%= form_for @user do |f| %>

 <%= f.label :username %><br>
 <%= f.text_field :username %></br>

 <%= f.label :email %><br>
 <%= f.text_field :email %></br>

 <%= f.label :bio %><br>
 <%= f.text_field :bio %></br>

 <%= f.label :favorite_game %><br>
 <%= f.text_field :favorite_game %></br>

 <%= f.label :password %><br>
 <%= f.password_field :password %></br>

   <br>

<%= f.submit "Create User" %>

<% end %>
