<h1>Join the Renecades Gameroom!</h1>
 <%= form_for @user do |f| %>

 <%= f.label :username, "Username:" %>
 <%= f.text_field :username %><br>

 <%= f.label :email, "Email:" %>
 <%= f.text_field :email %><br>

 <%= f.label :bio, "Bio:" %>
 <%= f.text_field :bio %><br>

 <%= f.label :favorite_game, "Favorite Game:" %>
 <%= f.text_field :favorite_game %><br>

 <%= f.label :password, "Password:" %>
 <%= f.password_field :password %><br>

   <br>

<%= f.submit "Create User" %>

<% end %>
