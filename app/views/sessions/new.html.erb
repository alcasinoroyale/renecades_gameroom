<h3> Welcome back to Renecades, Please Login to Play!</h3>
<div class="wrapper">
<%= form_for @user, url: sessions_path do |f| %>
 <%= f.label :username, "Username:" %><br>
<%= f.text_field :username %>
 <br>
 <%= f.label :password, "Password:" %><br>
<%= f.password_field :password %><br><br>
 <%= f.submit "Log In" %>
<% end %>
</div>
